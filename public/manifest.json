{
  "name": "vibey_lsp",
  "version": "1.0.0",
  "description": "",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Vibey LSP"
  },
  "background": {
    "service_worker": "src/serviceWorkers/hoverHintRetrievalServiceWorker.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/contentScripts/processCodeBlocks.ts"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["scripting"]
}
