{
  "name": "vibey_lsp",
  "version": "1.0.0",
  "description": "",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Vibey LSP"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "src/colorFunctionality/serviceWorker/serviceWorker.ts",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["scripting", "storage"],
  "optional_host_permissions": ["<all_urls>"]
}
